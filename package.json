{
  "name": "@datalayer-examples/jupyter-react-webpack",
  "version": "0.1.0",
  "description": "Jupyter React - Webpack Example",
  "license": "MIT",
  "private": true,
  "main": "lib/index.js",
  "files": [
    "lib",
    "style"
  ],
  "keywords": [
    "jupyter",
    "react",
    "jupyterlab"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/datalayer-examples/jupyter-react-webpack-example.git"
  },
  "scripts": {
    "build": "gulp resources-to-lib && tsc && webpack",
    "build:prod": "gulp resources-to-lib && tsc",
    "clean": "rimraf lib build dist tsconfig.tsbuildinfo",
    "start": "run-p -c 'start:*'",
    "start_:jupyter": "make start-jupyter-server",
    "start:webpack": "webpack serve",
    "build:tsc:watch:tsc": "tsc --watch",
    "build:tsc:watch:res": "gulp resources-to-lib-watch",
    "watch": "tsc --watch",
    "kill": "./dev/sh/kill-jupyter-server.sh || true"
  },
  "dependencies": {
    "@datalayer/jupyter-react": "0.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@types/codemirror": "5.60.4",
    "@types/marked": "4.0.1",
    "@types/node": "18.11.9",
    "@types/plotly.js": "1.54.11",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/styled-components": "5.1.26",
    "@types/uuid": "8.3.0",
    "babel-loader": "8.3.0",
    "buffer": "6.0.3",
    "bundle-loader": "0.5.6",
    "css-loader": "5.2.4",
    "file-loader": "~5.0.2",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "gulp-filter": "6.0.0",
    "gulp-watch": "5.0.1",
    "html-webpack-plugin": "5.3.1",
    "html-webpack-tags-plugin": "2.0.17",
    "ignore-loader": "0.1.2",
    "npm-run-all": "4.1.5",
    "process": "0.11.10",
    "raw-loader": "4.0.2",
    "rimraf": "~3.0.0",
    "stream-browserify": "2.0.2",
    "style-loader": "2.0.0",
    "svg-url-loader": "7.1.1",
    "typescript": "5.0.3",
    "url-loader": "~3.0.0",
    "watch": "~1.0.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  }
}
